{
  "name": "@basoftvn/vnu-portal",
  "version": "0.1.2",
  "description": "Vietnam National University, Ha Noi - Interactive CLI application for credit registration",
  "author": "Hai Binh Ng. <binhmattroi@gmail.com>",
  "private": false,
  "license": "GPL-3.0-or-later",
  "bin": {
    "vnu-portal": "cli"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prepublishOnly": "npm run build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@supercharge/console-io": "^1.3.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "columnify": "^1.6.0",
    "nestjs-command": "^3.1.2",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "string-argv": "^0.3.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/columnify": "^1.5.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/request": "^2.48.8",
    "@types/request-promise-native": "^1.0.18",
    "@types/supertest": "^2.0.11",
    "@types/yargs": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "main": "dist/main.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basoftvn/vnu-portal.git"
  },
  "types": "./dist/main.d.ts",
  "bugs": {
    "url": "https://github.com/basoftvn/vnu-portal/issues"
  },
  "homepage": "https://github.com/basoftvn/vnu-portal#readme",
  "publishConfig": {
    "access": "public"
  }
}
